<Window x:Class="KcAgentBrowser.DebugWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="DebugWindow" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <ListBox x:Name="lst"
                 SelectionChanged="lst_SelectionChanged" 
                 />
        <TextBox x:Name="txt" Grid.Row="1" />

    </Grid>
</Window>
